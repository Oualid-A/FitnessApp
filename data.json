{
    "programs": [
        {
            "name": "Beginner's Bodyweight Workout",
            "description": "A beginner-friendly workout routine focusing on bodyweight exercises.",
            "duration": "4 weeks",
            "level": "Beginner",
            "type": "Bodyweight",
            "focus": [
                "Full Body"
            ],
            "workouts": [
                {
                    "name": "Day 1",
                    "description": "Full body workout targeting major muscle groups.",
                    "exercises": [
                        {
                            "name": "Push-ups",
                            "sets": 3,
                            "reps": "10-12"
                        },
                        {
                            "name": "Bodyweight Squats",
                            "sets": 3,
                            "reps": "12-15"
                        },
                        {
                            "name": "Plank",
                            "sets": 3,
                            "duration": "30 seconds"
                        }
                    ]
                },
                {
                    "name": "Day 2",
                    "description": "Upper body focus with pushing and pulling movements.",
                    "exercises": [
                        {
                            "name": "Pull-ups or Assisted Pull-ups",
                            "sets": 3,
                            "reps": "6-8"
                        },
                        {
                            "name": "Dips or Bench Dips",
                            "sets": 3,
                            "reps": "8-10"
                        },
                        {
                            "name": "Bodyweight Rows (or Inverted Rows)",
                            "sets": 3,
                            "reps": "10-12"
                        }
                    ]
                }
            ]
        },
        {
            "name": "Intermediate Strength Training",
            "description": "An intermediate-level strength training program using barbells and dumbbells.",
            "duration": "8 weeks",
            "level": "Intermediate",
            "type": "Strength Training",
            "focus": [
                "Upper Body",
                "Lower Body"
            ],
            "workouts": [
                {
                    "name": "Day 1: Upper Body",
                    "description": "Focus on chest, back, shoulders, and arms.",
                    "exercises": [
                        {
                            "name": "Bench Press",
                            "sets": 4,
                            "reps": "8-10"
                        },
                        {
                            "name": "Barbell Rows",
                            "sets": 4,
                            "reps": "8-10"
                        },
                        {
                            "name": "Overhead Press",
                            "sets": 4,
                            "reps": "8-10"
                        }
                    ]
                },
                {
                    "name": "Day 2: Lower Body",
                    "description": "Focus on legs and core.",
                    "exercises": [
                        {
                            "name": "Squats",
                            "sets": 4,
                            "reps": "8-10"
                        },
                        {
                            "name": "Deadlifts",
                            "sets": 4,
                            "reps": "6-8"
                        },
                        {
                            "name": "Lunges",
                            "sets": 4,
                            "reps": "10-12 each leg"
                        }
                    ]
                }
            ]
        }
    ]
}